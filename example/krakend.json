          "http_method": "POST"
        }
      },
      "backend": [
        {
          "url_pattern": "/api/vbeta/user/all/",
          "encoding": "safejson",
          "method": "GET",
          "host": [
            "http://170.78.59.203:14000"
          ],
          "disable_host_sanitize": true
        }
      ]
    },
    {
      "endpoint": "error/user/all/",
      "method": "GET",
      "output_encoding": "json",

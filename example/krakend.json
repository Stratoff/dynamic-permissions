{
  "version": 2,
  "timeout": "300000ms",
  "cache_ttl": "300s",
  "output_encoding": "json",
  "name": "Ariatel ATS",
  "port": 20000,
  "read_timeout": "100s",
  "read_header_timeout": "100s",
  "write_timeout": "100s",
  "idle_timeout": "100s",
  "extra_config": {
    "github_com/devopsfaith/krakend-cors": {
      "allow_origins": [
